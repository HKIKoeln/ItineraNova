    <xs:element name="chDesc">
        <xs:annotation>
            <xs:documentation xml:lang="de">Faßt die Metadaten einer Urkunde zusammen.
Metadatenelemente</xs:documentation>
            <xs:documentation xml:lang="en">contains the meta-data for the charter</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
