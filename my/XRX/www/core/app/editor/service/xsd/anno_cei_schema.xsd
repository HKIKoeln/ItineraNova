            </xs:sequence>
            <xs:attribute ref="type"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="dispositio">
        <xs:annotation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Formular</menu>
                <caption xml:lang="de">Dispositio</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Dispositio</xs:appinfo>
            <xs:documentation xml:lang="de">enthält die eigentlichen Rechtsverfügungen</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="Originalbefund"/>
                    <xs:group ref="Sachliches"/>
                    <xs:element ref="setPhrase"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="type"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="corroboratio">
        <xs:annotation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Formular</menu>
                <caption xml:lang="de">Corroboratio</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Corroboratio</xs:appinfo>
            <xs:documentation xml:lang="de">kündigt die Beglaubigungsmittel an</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="Originalbefund"/>
                    <xs:group ref="Sachliches"/>
                    <xs:element ref="setPhrase"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="type"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="sanctio">
        <xs:annotation>
            <xs:documentation xml:lang="de">Die Textpassage, in der eine Strafe bei Brechen des Urkundeninhalts angedroht wird.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Formular</menu>
                <caption xml:lang="de">Sanctio</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Sanctio</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="Originalbefund"/>
                    <xs:group ref="Sachliches"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="type"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="subscriptio">
        <xs:annotation>
            <xs:documentation xml:lang="de">Unterschriftsbestandteile, wie echte Unterschriftszeilen, Signumzeilen, Gegenzeichnungen durch Kanzleimitarbeiter usw.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Formular</menu>
                <caption xml:lang="de">Unterschriften</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Unterschriften</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="Originalbefund"/>
                    <xs:group ref="Sachliches"/>
                    <xs:element minOccurs="0" maxOccurs="unbounded" ref="setPhrase"/>
                    <xs:element minOccurs="0" maxOccurs="unbounded" ref="notariusSub"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="type">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Die Unterscheidung in "Signumzeile", "Rekognitionszeile", "Notarsunterschrift" (vgl. notariusSub: [strict!]) etc. Der verwendete Sprachgebrauch sollte sich an das VID anlehnen oder in der encodingDesc beschrieben werden.</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="datatio">
        <xs:annotation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Formular</menu>
                <caption xml:lang="de">Datierung</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Datierung</xs:appinfo>
            <xs:documentation xml:lang="de">Datierung der Urkunde</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="Originalbefund"/>
                    <xs:group ref="Sachliches"/>
                    <xs:element ref="setPhrase"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="type"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="apprecatio">
        <xs:annotation>
            <xs:documentation xml:lang="de">eine gebetsartige abschließende Floskel</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="Originalbefund"/>
                    <xs:group ref="Sachliches"/>
                    <xs:element ref="setPhrase"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="type"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="setPhrase">
        <xs:annotation>
            <xs:documentation xml:lang="de">eine stark festgelegte Textpassage, die sich nur durch die individuellen Bedingungen (Zeit, Ort, Person, syntaktische Einbindung) verändert, die aber keine strukturierende Funktion im Sinne des Urkundenformulars hat. Soweit möglich sollte sich der Inhalt des Attributs "type" an der Terminologie des "Vocabulaire Internationale de Diplomatique" orientieren.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Formular</menu>
                <caption xml:lang="de">Klauser, Binnenformel</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Klauser, Binnenformel</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="Originalbefund"/>
                    <xs:group ref="Sachliches"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="type"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="notariusSub">
        <xs:annotation>
            <xs:documentation xml:lang="de">Die Notarsunterschrift</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Formular</menu>
                <caption xml:lang="de">Notarsunterschrift</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Notarsunterschrift</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="Originalbefund"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="notariusSign">
        <xs:annotation>
            <xs:documentation xml:lang="de">Das Notarszeichen</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:anySimpleType">
                    <xs:attribute ref="key"/>
                    <xs:attributeGroup ref="standard"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:group name="Originalbefund">
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="transcription"/>
                <xs:element ref="hi"/>
                <xs:element ref="pb"/>
                <xs:element ref="lb"/>
                <xs:element ref="sup">
                    <xs:annotation>
                        <!--xs:appinfo source="EditMOM.xsd">
                            <present>no</present>
                        </xs:appinfo-->
                    </xs:annotation>
                </xs:element>
                <xs:element ref="pict"/>
            </xs:choice>
        </xs:sequence>
    </xs:group>
    <xs:group name="transcription">
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="expan"/>
                <xs:element ref="add"/>
                <xs:element ref="del"/>
                <xs:element ref="corr"/>
                <xs:element ref="space"/>
                <xs:element ref="damage"/>
                <xs:element ref="c"/>
                <xs:element ref="sic"/>
                <xs:element ref="handShift"/>
            </xs:choice>
        </xs:sequence>
    </xs:group>
    <xs:element name="space">
        <xs:annotation>
            <xs:documentation xml:lang="de">Ein signifikant großer unbeschriebener Raum in der Transkriptionsvorlage. Der Leerraum wird mit den Attributen "dim" (horizontal/vertical) und "extent" (Größe) beschrieben: Z.B. dim="vertical" extent="4 Zeilen".
Ist die Lücke durch eine Rasur entstanden, ist ein leeres Element "del" vorzuziehen.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Original</menu>
                <caption xml:lang="de">Unbeschrieben</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Unbeschrieben</xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="dim"/>
            <xs:attribute name="extent"/>
            <xs:attributeGroup ref="transcription"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="damage">
        <xs:annotation>
            <xs:documentation xml:lang="de">enthält beschädigte Passagen, d.h. Stellen, in denen die physischen Eigenschaften des Textes ihn nur schwer oder gar nicht entzifferbar machen.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Original</menu>
                <caption xml:lang="de">Beschädigte Stelle</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Beschädigte Stelle</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="expan"/>
                    <xs:element ref="c"/>
                    <xs:element ref="lb"/>
                    <xs:element ref="pb"/>
                    <xs:element ref="handShift"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute name="extent" type="xs:anySimpleType"/>
            <xs:attribute name="degree" type="xs:anySimpleType"/>
            <xs:attribute name="agent" type="xs:anySimpleType"/>
            <xs:attribute ref="certainty"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="handShift">
        <xs:annotation>
            <xs:documentation xml:lang="de">ein Handwechsel im Dokument. Das Attribut "hand" benennt die Hand des folgenden Textes, falls z.B. über eine Referenz auf eine Liste der Schreiber (Element handList) möglich.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Original</menu>
                <caption xml:lang="de">Handwechsel</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Handwechsel</xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute ref="hand"/>
            <xs:attribute ref="certainty"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="app">
        <xs:annotation>
            <xs:documentation xml:lang="de">Varianten anderer kopialer Überlieferungen werden über das Referenzwort als Element "lem" und die Varianten als Elemente "rdg" mit dem Attribut "sigil" für die Sigle.</xs:documentation>
            <xs:documentation xml:lang="en">note in a critical apparatus listing variants</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <present>no</present>
            </xs:appinfo-->
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:element name="lem">
                    <xs:annotation>
                        <xs:documentation xml:lang="de">Die im Haupttext erscheinende Version.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType mixed="true">
                        <xs:sequence>
                            <xs:choice minOccurs="0" maxOccurs="unbounded">
                                <xs:group ref="Sachauszeichnung"/>
                                <xs:group ref="biblArch"/>
                                <xs:group ref="layout"/>
                                <xs:group ref="reference"/>
                                <xs:group ref="textualElements"/>
                            </xs:choice>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element maxOccurs="unbounded" name="rdg">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">reading</xs:documentation>
                    </xs:annotation>
                    <xs:complexType mixed="true">
                        <xs:sequence>
                            <xs:choice minOccurs="0" maxOccurs="unbounded">
                                <xs:group ref="biblArch"/>
                                <xs:group ref="layout"/>
                                <xs:group ref="reference"/>
                            </xs:choice>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="expan">
        <xs:annotation>
            <xs:documentation xml:lang="de">Der ausgeschriebene Text einer Abkürzung.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Original</menu>
                <caption xml:lang="de">Abkürzung</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Abkürzung</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="c"/>
                    <xs:element ref="lb"/>
                    <xs:element ref="pb"/>
                    <xs:element ref="add"/>
                    <xs:element ref="corr"/>
                    <xs:element ref="sic"/>
                    <xs:element ref="damage"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="type"/>
            <xs:attribute ref="abbr">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Die Buchstaben der Abkürzung</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute ref="certainty"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="sic">
        <xs:annotation>
            <xs:documentation xml:lang="de">Ein Text, der augenfällige Fehler aufweist, aber in der Vorlage so steht. Aber auch für den Text, der vor einer Korrektur stand.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Original</menu>
                <caption xml:lang="de">Fehler</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Fehler</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="expan"/>
                    <xs:element ref="c"/>
                    <xs:element ref="lb"/>
                    <xs:element ref="add"/>
                    <xs:element ref="handShift"/>
                    <xs:group ref="Referenz"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute name="corr"/>
            <xs:attribute ref="hand"/>
            <xs:attribute ref="certainty"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="corr">
        <xs:annotation>
            <xs:documentation xml:lang="de">Eine Textpassage, die im Text zunächst anders lautete und dann mit dem Inhalt des Elements korrigiert worden ist.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Original</menu>
                <caption xml:lang="de">Korrektur</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Korrektur</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="expan"/>
                    <xs:element ref="sic"/>
                    <xs:element ref="lb"/>
                    <xs:element ref="pb"/>
                    <xs:element ref="c"/>
                    <xs:element ref="damage"/>
                    <xs:element ref="handShift"/>
                    <xs:group ref="Sachauszeichnung"/>
                    <xs:group ref="Referenz"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute name="sic"/>
            <xs:attributeGroup ref="transcription"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="del">
        <xs:annotation>
            <xs:documentation xml:lang="de">Ein noch lesbarer Text der im Original getilgt worden ist (z.B. durch Durchstreichen, Unterpunkten, Rasur).</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Original</menu>
                <caption xml:lang="de">Tilgung</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Tilgung</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="expan"/>
                    <xs:element ref="add"/>
                    <xs:element ref="c"/>
                    <xs:element ref="damage"/>
                    <xs:element ref="lb"/>
                    <xs:element ref="pb"/>
                    <xs:element ref="sic"/>
                    <xs:group ref="Referenz"/>
                </xs:choice>
            </xs:sequence>
            <xs:attributeGroup ref="transcription"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="add">
        <xs:annotation>
            <xs:documentation xml:lang="de">Nachträgliche Hinzufügungen im Text, die nicht anderen Text ersetzen.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Original</menu>
                <caption xml:lang="de">Hinzufügung</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Hinzufügung</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="expan"/>
                    <xs:element ref="c"/>
                    <xs:element ref="lb"/>
                    <xs:element ref="pb"/>
                    <xs:element ref="sic"/>
                    <xs:element ref="corr"/>
                    <xs:element ref="del"/>
                    <xs:element ref="damage"/>
                    <xs:element ref="handShift"/>
                    <xs:group ref="Sachauszeichnung"/>
                    <xs:group ref="Referenz"/>
                </xs:choice>
            </xs:sequence>
            <xs:attributeGroup ref="transcription"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="pict">
        <xs:annotation>
            <xs:documentation xml:lang="de">graphische, zeichenartige Elemente im Text. Mit Hilfe des type-Attribut genauer zu bestimmen, mit entity oder URL auf eine externe Art der Darstellung zu verweisen.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Original</menu>
                <caption xml:lang="de">Graphisches Zeichen</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Graphisches Zeichen</xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="URL" type="xs:anySimpleType"/>
            <xs:attributeGroup ref="transcription"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:group name="layout">
        <xs:annotation>
            <xs:documentation xml:lang="de">Diese ganze Gruppe ist auf ihre Relevanz hin zu überprüfen, da sie graphische nicht semantische Informationen auszuzeichnen scheint. [strict!]</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="hi"/>
                <xs:element ref="lb"/>
                <xs:element ref="pb"/>
                <xs:element ref="sup"/>
                <xs:element ref="c"/>
            </xs:choice>
        </xs:sequence>
    </xs:group>
    <xs:element name="sup">
        <xs:annotation>
            <xs:documentation xml:lang="de">Ein hochgestelltes Zeichen, dessen logische Funktion "hochgestellt" ist, macht das Sinn? Evtl. nur cei_MOM spezifisch! [strict!] Der in Urkunden relevante Fall sind die Ordinalzahlen, in denen das hochgestellte Zeichen nämlich nicht für eine Abkürzung steht, sondern den Bezug einer Zahlenangabe über die Endung markiert. Die TEI sähe für Ordinalzahlen das Element "num", das mit den Attributen type und value die Funktion als Ordinalzahl und den eigentlichen Wert notiert. Es bleibt dabei jedoch unklar, wie zwischen dem Zahlzeichen und seiner Markierung als Ordinalzahl unterschieden werden kann.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Original</menu>
                <caption xml:lang="de">SUP</caption>
                </xs:appinfo-->
            <xs:appinfo source="EditVDU">Hochgestellt</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:attribute name="class"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="lb">
        <xs:annotation>
            <xs:documentation xml:lang="de">Zeilenwechsel in der Vorlage.</xs:documentation>
            <xs:documentation xml:lang="en">Line break in the source.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Original</menu>
                <caption xml:lang="de">Zeilenwechsel</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Zeilenwechsel</xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="pb">
        <xs:annotation>
            <xs:documentation xml:lang="de">Seitenwechsel in der Vorlage.</xs:documentation>
            <xs:documentation xml:lang="en">Page break in the source.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Original</menu>
                <caption xml:lang="de">Seitenwechsel</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Seitenwechsel in der Vorlage</xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="c">
        <xs:annotation>
            <xs:documentation xml:lang="de">Ein einzelnes Zeichen in der Vorlage, das nicht mit dem gewählten Zeichensatz dargestellt werden kann. Die Art und Weise der Darstellung und Verarbeitung ist in der encodingDesc zu beschreiben.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Original</menu>
                <caption xml:lang="de">Sonderzeichen</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Sonderzeichen</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:attribute ref="type">
                <xs:annotation>
                    <xs:documentation xml:lang="de">Der Name des Zeichens, wie er in der encodingDesc/profileDesc auftaucht und mit einer Darstellungsform versehen wird.</xs:documentation>
                    <!-- ACHTUNG VDU ANNO FÜR ATTRIBUTE -->
                    <!--xs:appinfo source="EditMOM.xsd">
                        <caption>Beschreibung</caption>
                    </xs:appinfo-->
                </xs:annotation>
            </xs:attribute>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="hi">
        <xs:annotation>
            <xs:documentation xml:lang="de">durch graphische Merkmale hervorgehobene Textpassagen, die keine rein inhaltliche Funktion haben, z.B. vergrößerte Initialen, gesperrte Texte, Texte zwischen Referenzpunkten, Elongata</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Original</menu>
                <overlap>True</overlap>
                <caption xml:lang="de">graphische Hervorhebung</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">graphische Hervorhebung</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="Originalbefund"/>
                    <xs:group ref="Sachliches"/>
                    <xs:group ref="Formular"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="rend"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:group name="reference">
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="ptr"/>
                <xs:element ref="note"/>
                <xs:element ref="figure"/>
                <xs:element ref="anchor"/>
                <xs:element ref="ref"/>
            </xs:choice>
        </xs:sequence>
    </xs:group>
    <xs:group name="Referenz">
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="anchor"/>
                <xs:element ref="ref"/>
            </xs:choice>
        </xs:sequence>
    </xs:group>
    <xs:element name="ptr">
        <xs:annotation>
            <xs:documentation xml:lang="de">Ein leeres Element, das auf ein anderes Element im Text verweist, das ein id-Attribut besitzt. Die ID des Zieles wird im Attribut target notiert. Verweise auf Elemente außerhalb des aktuellen Dokuments sind mit dem Element xptr zu verwirklichen. Ist der Verweis als Textpassage im aktuellen Element ausformuliert, ist das Element "ref" zu verwenden.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attributeGroup ref="reference"/>
                    <xs:attributeGroup ref="standard"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="ref">
        <xs:annotation>
            <xs:documentation xml:lang="de">Das Element enthält eine Textpassage, die auf eine anderes Element im Text verweist, das ein id-Attribut besitzt. Die ID des Zieles wird im Attribut "target" angegeben. Verweise auf Elemente außerhalb des aktuellen Dokuments sind mit dem Element xref zu verwirklichen. Ersetzt das Tag einen textlichen Verweis bzw. wird der Verweistext automatisch erzeugt, ist das Element "ptr" zu verwenden.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Referenz</menu>
                <caption xml:lang="de">Verweis</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Verweis</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:simpleContent>
                <xs:extension base="xs:anySimpleType">
                    <xs:attributeGroup ref="reference"/>
                    <xs:attribute ref="lang"/>
                    <xs:attributeGroup ref="standard"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="note">
        <xs:annotation>
            <xs:documentation xml:lang="de">Ein ergänzender Text zu einer bestimmten Textstelle, der im Druck gewöhnlich als Marginalie, Fuß- oder Endnote dargestellt wird.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Referenz</menu>
                <caption xml:lang="de">NOTE</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">NOTE</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="Originalbefund"/>
                    <xs:group ref="Sachliches"/>
                    <xs:element ref="bibl"/>
                    <xs:element ref="archIdentifier"/>
                    <xs:element ref="msIdentifier"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute name="place"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:group name="textualElements">
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element name="head" type="xs:string">
                    <xs:annotation>
                        <xs:documentation xml:lang="de">Eine Überschrift</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element ref="h1"/>
                <xs:element ref="h2"/>
                <xs:element ref="p"/>
                <xs:element ref="list"/>
                <xs:element ref="foreign"/>
                <xs:element ref="cit"/>
                <xs:element ref="quote"/>
                <xs:element ref="div"/>
            </xs:choice>
        </xs:sequence>
    </xs:group>
    <xs:element name="div">
        <xs:annotation>
            <xs:documentation xml:lang="de">Jede beliebige Art von Textabschnitt, die einen inhaltlichen Sinnabschnitt von mindestens einem Absatz umschließt und nicht durch speziellere Elemente (z.B. Formularteile, abstract) beschrieben werden kann.</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="textualElements"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="type"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="divNotes">
        <xs:annotation>
            <xs:documentation xml:lang="de">Enthält die Fußnoten zur Urkunde.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Referenz</menu>
                <caption xml:lang="de">FUssNOTE</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">FUssNOTE</xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="note"/>
                </xs:choice>
            </xs:sequence>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="foreign">
        <xs:annotation>
            <xs:documentation xml:lang="de">Eine Textpassage, die nicht in der in der übergeordnet definierten Sprache formuliert ist (z.B. deutsche Namen in lateinischen Urkunden, eingeflochtene lateinische Textpassagen im deutschen Vorspann etc.)</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Sache</menu>
                <caption xml:lang="de">Anderssprachig</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Anderssprachig</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="Originalbefund"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="cit">
        <xs:annotation>
            <xs:documentation xml:lang="de">Ein wörtliches Zitat gemeinsam mit der Herkunftsangabe. Die Quelle dabei nicht auf explizit bibliographische Angaben beschränkt, sondern kann auch kanonische Referenzen (wie z.B. Bibelzitate) enthalten.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Sache</menu>
                <caption xml:lang="de">Zitat mit Quellangabe</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Zitat mit Quellangabe</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:element ref="quote"/>
                <xs:element ref="bibl"/>
            </xs:sequence>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="quote">
        <xs:annotation>
            <xs:documentation xml:lang="de">enthält ein wörtliches Zitat. Die TEI sieht das Element für intertextuelle Zitate vor. Alternativ und lauf Guidelines der TEI im Zweifelsfall vorzuziehen ist das Element "q" vor, das für intratextuelle Zitate zu verwenden ist. Da in Urkunden jedoch Zitate gewöhnlich aus einer externen Textquelle schöpfen und nicht einem anderen Sprecher im Text zugeordnet sind, ist in diesem Kontext das Element "quote" im Zweifelsfall vorzuziehen. [strict!] Zu diskutieren ist nur der Fall eines angekündigten wörtlichen Zitats, z.B: wir bestätigen die Urkunde unseres Vaters die folgenden Wortlaut hat: ....</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Sache</menu>
                <caption xml:lang="de">Zitat</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Zitat</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="Originalbefund"/>
                    <xs:group ref="Sachauszeichnung"/>
                    <xs:element ref="foreign"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="type"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="h1">
        <xs:complexType mixed="true">
            <xs:simpleContent>
                <xs:extension base="xs:anySimpleType">
                    <xs:attributeGroup ref="standard"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="h2">
        <xs:complexType mixed="true">
            <xs:simpleContent>
                <xs:extension base="xs:anySimpleType">
                    <xs:attributeGroup ref="standard"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="list">
        <xs:annotation>
            <xs:documentation xml:lang="de">Eine Liste</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence maxOccurs="unbounded">
                <xs:element ref="item"/>
            </xs:sequence>
            <xs:attribute ref="type"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="item">
        <xs:annotation>
            <xs:documentation xml:lang="de">Ein Listenpunkt</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="Originalbefund"/>
                    <xs:group ref="Sachliches"/>
                </xs:choice>
            </xs:sequence>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:group name="Sachliches">
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="Sachauszeichnung"/>
                <xs:element ref="quote"/>
                <xs:element ref="cit"/>
                <xs:element ref="foreign"/>
                <xs:group ref="Referenz"/>
            </xs:choice>
        </xs:sequence>
    </xs:group>
    <xs:element name="measure">
        <xs:annotation>
            <xs:documentation xml:lang="de">Ein Maßangabe inkl. Maßeinheit. Das Attribut "value" enthält eine vereinheitliche Form der Angabe die aus einem numerischen Wert und einer Maßangabe besteht.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Sache</menu>
                <caption xml:lang="de">Maße</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Maße</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="num"/>
                    <xs:element ref="index"/>
                    <xs:group ref="Originalbefund"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="type"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="num">
        <xs:annotation>
            <xs:documentation xml:lang="de">Eine Zahlenangabe</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Sache</menu>
                <caption xml:lang="de">Zahl</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Zahl</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:attribute ref="type"/>
            <xs:attribute ref="value"/>
            <xs:attribute ref="lang"/>
            <xs:attribute ref="rend"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:group name="Sachauszeichnung">
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="persName"/>
                <xs:choice>
                    <xs:annotation>
                        <xs:documentation xml:lang="de">Geographische Angaben</xs:documentation>
                    </xs:annotation>
                    <xs:element ref="placeName"/>
                    <xs:element ref="geogName"/>
                </xs:choice>
                <xs:element ref="index"/>
                <xs:element ref="testis"/>
                <xs:choice>
                    <xs:annotation>
                        <xs:documentation xml:lang="de">Datumsangaben</xs:documentation>
                    </xs:annotation>
                    <xs:element ref="date"/>
                    <xs:element ref="dateRange"/>
                </xs:choice>
                <xs:element ref="num"/>
                <xs:element ref="measure"/>
            </xs:choice>
        </xs:sequence>
    </xs:group>
    <xs:group name="date">
        <xs:choice>
            <xs:element ref="date"/>
            <xs:element ref="dateRange"/>
        </xs:choice>
    </xs:group>
    <xs:element name="index">
        <xs:annotation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Sache</menu>
                <caption xml:lang="de">Sachindex</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Sachindex</xs:appinfo>
            <xs:documentation xml:lang="de">ein Eintrag für eine erschließende Liste von Stichwörtern (Register, Index)</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="foreign"/>
                    <xs:group ref="Originalbefund"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute name="indexName"/>
            <xs:attribute name="lemma"/>
            <xs:attribute name="sublemma"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="date">
        <xs:annotation>
            <xs:documentation xml:lang="de">Die Angabe eines spezifischen Tages in einem Kalender (=Datumsangabe).</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Sache</menu>
                <caption xml:lang="de">Datumsangabe</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Datumsangabe</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="foreign"/>
                    <xs:element ref="num"/>
                    <xs:group ref="Originalbefund"/>
                </xs:choice>
            </xs:sequence>
            <xs:attributeGroup ref="date"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="dateRange">
        <xs:annotation>
            <xs:documentation xml:lang="de">Ein Zeitraum, die Werte des kleinsten und des größten zutreffenden Datums in den Attributen "from" und "to"</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Sache</menu>
                <caption xml:lang="de">Zeitraumangabe</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Zeitraumangabe</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="foreign"/>
                    <xs:element ref="num"/>
                    <xs:group ref="Originalbefund"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute name="from">
                <xs:annotation>
                    <!--xs:appinfo source="EditMOM.xsd">
                        <caption>von</caption>
                    </xs:appinfo-->
                    <xs:appinfo source="EditVDU">von</xs:appinfo>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="to">
                <xs:annotation>
                    <!--xs:appinfo source="EditMOM.xsd">
                        <caption>von</caption>
                    </xs:appinfo-->
                    <xs:appinfo source="EditVDU">bis</xs:appinfo>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute ref="certainty"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="persName">
        <xs:annotation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Sache</menu>
                <caption xml:lang="de">Personenname</caption>
            </xs:appinfo-->
            <xs:documentation xml:lang="de">ein Personenname</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:choice>
                        <xs:annotation>
                            <xs:documentation xml:lang="de">Geographische Angaben</xs:documentation>
                        </xs:annotation>
                        <xs:element ref="persName"/>
                        <xs:element ref="placeName"/>
                        <xs:element ref="geogName"/>
                    </xs:choice>
                    <xs:element ref="foreign"/>
                    <xs:element ref="note"/>
                    <xs:group ref="Originalbefund"/>
                </xs:choice>
            </xs:sequence>
            <xs:attributeGroup ref="identification"/>
            <xs:attribute ref="lang"/>
            <xs:attribute ref="type"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="recipient">
        <xs:annotation>
            <xs:documentation xml:lang="de">Der Name der Person, die die Urkunde erhält (als Adressat oder als Begünstigter)</xs:documentation>
            <xs:documentation xml:lang="en">The name of the person who is receives the document.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Sache</menu>
                <caption xml:lang="de">Empfänger</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Empfänger</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="foreign"/>
                    <xs:element ref="persName"/>
                    <xs:choice>
                        <xs:annotation>
                            <xs:documentation xml:lang="de">Geographische Angaben</xs:documentation>
                        </xs:annotation>
                        <xs:element ref="placeName"/>
                        <xs:element ref="geogName"/>
                    </xs:choice>
                    <xs:group ref="Originalbefund"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="issuer">
        <xs:annotation>
            <xs:documentation xml:lang="de">Der Aussteller der Urkunde.</xs:documentation>
            <xs:documentation xml:lang="en">marks a person the CID describes as follows:
L'auteur d'un acte écrit est la personne au nom de qui cet acte est intitulé.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Sache</menu>
                <caption xml:lang="de">Aussteller</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Aussteller</xs:appinfo>
            <!--xs:appinfo source="EditVDU-Test" xmlns:xe="http://monasterium.net/ns/xe">
                <xe:label>Aussteller</xe:label>
                <xe:relevant></xe:relevant>
                <xe:required></xe:required>
                <xe:control>input</xe:control>
                <xe:appeareance></xe:appeareance>
            </xs:appinfo-->
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="persName"/>
                    <xs:choice>
                        <xs:annotation>
                            <xs:documentation xml:lang="de">Geographische Angaben</xs:documentation>
                        </xs:annotation>
                        <xs:element ref="placeName"/>
                        <xs:element ref="geogName"/>
                    </xs:choice>
                    <xs:element ref="foreign"/>
                    <xs:element ref="note"/>
                    <xs:group ref="Originalbefund"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="testis">
        <xs:annotation>
            <xs:documentation xml:lang="de">Ein einzelner Zeuge.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Sache</menu>
                <caption xml:lang="de">Zeuge</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Zeuge</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="foreign"/>
                    <xs:group ref="Originalbefund"/>
                    <xs:choice>
                        <xs:annotation>
                            <xs:documentation xml:lang="de">Geographische Angaben</xs:documentation>
                        </xs:annotation>
                        <xs:element ref="placeName"/>
                        <xs:element ref="geogName"/>
                    </xs:choice>
                    <xs:element ref="persName"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="region">
        <xs:annotation>
            <xs:documentation>der Name einer Region, eines Bundeslandes u.ä.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Referenz</menu>
                <caption xml:lang="de">Land</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Land</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:attribute ref="type"/>
            <xs:attributeGroup ref="geog"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="country">
        <xs:annotation>
            <xs:documentation>der Name eines völkerrechtlich selbständigen Staates (z.B. Deutschland, Ungarn ...)</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Referenz</menu>
                <caption xml:lang="de">Staat</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Staat</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:attribute ref="type"/>
            <xs:attributeGroup ref="geog"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="settlement">
        <xs:annotation>
            <xs:documentation>der Name einer Siedlung (Dorf, Stadt ...)</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Referenz</menu>
                <caption xml:lang="de">Ort</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Ort</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:attribute ref="type"/>
            <xs:attributeGroup ref="geog"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="placeName">
        <xs:annotation>
            <xs:documentation xml:lang="de">Enthält geographische Angaben, die sich auf menschliche Siedlung oder politische Einheiten beziehen, d.h. Orte, Hofnamen, Klöster, Burgen, Bundesländer, Herrschaften, Staaten etc.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Sache</menu>
                <caption xml:lang="de">Ortsname</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Ortsname</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="foreign"/>
                    <xs:element ref="note"/>
                    <xs:group ref="Originalbefund"/>
                </xs:choice>
            </xs:sequence>
            <xs:attributeGroup ref="geog"/>
            <xs:attribute ref="type"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="geogName">
        <xs:annotation>
            <xs:documentation xml:lang="de">Enhält naturräumliche geographische Angaben wie Bergnamen, Flußnamen, Landstriche, die keine politische Einheit bilden.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Sache</menu>
                <caption xml:lang="de">Geographischer Name</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Geographischer Name</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="foreign"/>
                    <xs:element ref="note"/>
                    <xs:group ref="Originalbefund"/>
                </xs:choice>
            </xs:sequence>
            <xs:attributeGroup ref="geog"/>
            <xs:attribute ref="type"/>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:group name="BibliographicInformation">
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element minOccurs="0" name="author">
                    <xs:annotation>
                        <xs:documentation xml:lang="de">Der Autor eines Textes. Insbesondere im Rahmen einer bibliographischen Angabe gebraucht.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:simpleContent>
                            <xs:extension base="xs:string">
                                <xs:attributeGroup ref="standard"/>
                            </xs:extension>
                        </xs:simpleContent>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="title">
                    <xs:complexType>
                        <xs:simpleContent>
                            <xs:extension base="xs:string">
                                <xs:attributeGroup ref="standard"/>
                            </xs:extension>
                        </xs:simpleContent>
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="imprint">
                    <xs:complexType mixed="true">
                        <xs:sequence>
                            <xs:choice minOccurs="0" maxOccurs="unbounded">
                                <xs:element ref="scope"/>
                                <xs:element name="pubPlace">
                                    <xs:complexType>
                                        <xs:simpleContent>
                                            <xs:extension base="xs:string">
                                                <xs:attributeGroup ref="standard"/>
                                            </xs:extension>
                                        </xs:simpleContent>
                                    </xs:complexType>
                                </xs:element>
                                <xs:group ref="date"/>
                            </xs:choice>
                        </xs:sequence>
                        <xs:attributeGroup ref="standard"/>
                    </xs:complexType>
                </xs:element>
                <xs:element ref="scope"/>
            </xs:choice>
        </xs:sequence>
    </xs:group>
    <xs:element name="name">
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:group ref="layout"/>
            </xs:sequence>
            <xs:attributeGroup ref="standard"/>
            <xs:attributeGroup ref="reference"/>
        </xs:complexType>
    </xs:element>
    <xs:group name="biblArch">
        <xs:sequence>
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="country"/>
                <xs:element ref="region"/>
                <xs:element ref="settlement"/>
                <xs:element ref="arch"/>
                <xs:element ref="archFond"/>
                <xs:element ref="idno"/>
                <xs:element ref="provenance"/>
                <xs:element ref="bibl"/>
                <xs:element ref="msIdentifier"/>
                <xs:element ref="archIdentifier"/>
                <xs:element ref="repository"/>
            </xs:choice>
        </xs:sequence>
    </xs:group>
    <xs:element name="bibl">
        <xs:annotation>
            <xs:documentation xml:lang="de">Bibliographische Angaben. Es ist zu diskutieren, ob das Element auch für kanonische Angaben (wie z.B. Bibelzitate) verwendet werden kann, oder ob dafür ein eigenes Element notwendig ist. [strict!]</xs:documentation>
            <xs:documentation xml:lang="en">Bibliographic informations</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <liste>simple</liste>
                <menu>Referenz</menu>
                <caption xml:lang="de">Bibliogr. Angabe</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Bibliogr. Angabe</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:choice>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="layout"/>
                    <xs:element ref="foreign"/>
                    <xs:element ref="quote"/>
                    <xs:element ref="scope">
                        <xs:annotation>
                            <!--xs:appinfo source="EditMOM.xsd">
                                <present>no</present>
                            </xs:appinfo-->
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
            </xs:choice>
            <xs:attribute ref="lang"/>
            <xs:attributeGroup ref="standard"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="title">
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attributeGroup ref="standard"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="scope">
        <xs:annotation>
            <xs:documentation xml:lang="de">Umfang einer bibliographischen Angabe, insbesondere in einem übergeordneten bibliographsichen Zusammenhang. Z.B. die Seitenzahlen eines Aufsatzes in einem Sammelband, die Fundstelle einer Urkunde in einem Kopialbuch.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <caption xml:lang="de">Blätter/Seiten</caption>
                <menu>Referenz</menu>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Blätter/Seiten</xs:appinfo>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:simpleContent>
                <xs:extension base="xs:anySimpleType">
                    <xs:attribute ref="lang"/>
                    <xs:attributeGroup ref="standard"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="anchor">
        <xs:annotation>
            <xs:documentation xml:lang="de">Leeres Element, das als Zielpunkt einer Referenz dient.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <menu>Referenz</menu>
                <caption xml:lang="de">Zielpunkt</caption>
                </xs:appinfo-->
            <xs:appinfo source="EditVDU">Zielpunkt</xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attributeGroup ref="standard"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:element name="a">
        <xs:annotation>
            <xs:documentation>CEI_MOM: Nur für vorläufigen Gebrauch um Konversionen aus HTML zu erleichtern!!!! [strict!] </xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <present>no</present>
            </xs:appinfo-->
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:simpleContent>
                <xs:extension base="xs:anySimpleType">
                    <xs:attribute name="href"/>
                    <xs:attribute name="name"/>
                    <xs:attribute name="target"/>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>
    <xs:attributeGroup name="reference">
        <xs:attribute ref="type"/>
        <xs:attribute ref="resp"/>
        <xs:attribute ref="target"/>
        <xs:attribute ref="key"/>
    </xs:attributeGroup>
    <xs:attribute name="target" type="xs:anySimpleType">
        <xs:annotation>
            <!--xs:appinfo source="EditMOM.xsd">
                <caption xml:lang="de">Ziel</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Ziel</xs:appinfo>
        </xs:annotation>
    </xs:attribute>
    <xs:attributeGroup name="certain">
        <xs:attribute ref="certainty"/>
        <xs:attribute ref="resp"/>
    </xs:attributeGroup>
    <xs:attribute name="certainty" type="xs:anySimpleType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Für Genauigkeitsangaben z.B. bei Datierungen, werden mit Schlüsselwörtern (zu benennen in der encodingDesc) oder kurzen Beschreibungen charakterisiert werden. auch für Fälschungsmarkierungen? [strict!]</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <caption xml:lang="de">Zuverlässigkeit</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Zuverlässigkeit</xs:appinfo>
        </xs:annotation>
    </xs:attribute>
    <xs:attributeGroup name="identification">
        <xs:attribute ref="reg">
            <xs:annotation>
                <xs:documentation xml:lang="de">Normalisierte Version</xs:documentation>
                <!--xs:appinfo source="EditMOM.xsd">
                    <caption xml:lang="de">Normalform</caption>
                </xs:appinfo-->
                <xs:appinfo source="EditVDU">Normalform</xs:appinfo>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute ref="key"/>
        <xs:attribute ref="certainty"/>
    </xs:attributeGroup>
    <xs:attribute name="reg"/>
    <xs:attribute name="resp" type="xs:anySimpleType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Der verantwortliche Bearbeiter für die normalisierten Angaben und die Angaben zum Befund.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <caption xml:lang="de">Verantwortlich</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Verantwortlich</xs:appinfo>
        </xs:annotation>
    </xs:attribute>
    <xs:attribute name="type" type="xs:anySimpleType"/>
    <xs:attribute name="picture" type="xs:anySimpleType"/>
    <xs:attributeGroup name="geog">
        <xs:attribute name="existent"/>
        <xs:attributeGroup ref="identification"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="gap">
        <xs:attribute name="reason"/>
        <xs:attribute name="hand"/>
        <xs:attribute name="extent"/>
    </xs:attributeGroup>
    <xs:attribute name="function" type="xs:anySimpleType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Funktion des Tags im besonderen Kontext: z.B. Datum als ... ist so eigentlich nicht richtig.</xs:documentation>
        </xs:annotation>
    </xs:attribute>
    <xs:attribute name="key" type="xs:anySimpleType">
        <xs:annotation>
            <xs:documentation xml:lang="de">Ein eindeutiger Bezeichner, der in einer externen Liste aufgelöst werden kann.</xs:documentation>
            <!--xs:appinfo source="EditMOM.xsd">
                <present>no</present>
                <caption xml:lang="de">Schlüsselwert</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Schlüsselwert</xs:appinfo>
        </xs:annotation>
    </xs:attribute>
    <xs:attributeGroup name="date">
        <xs:attribute ref="value"/>
        <xs:attribute ref="certainty"/>
    </xs:attributeGroup>
    <xs:attribute name="value" type="xs:anySimpleType"/>
    <xs:attributeGroup name="abbreviation">
        <xs:attribute ref="expan"/>
        <xs:attribute ref="abbr"/>
    </xs:attributeGroup>
    <xs:attribute name="abbr" type="xs:anySimpleType">
        <xs:annotation>
            <!--xs:appinfo source="EditMOM.xsd">
                <caption xml:lang="de">Abkürzung</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Abkürzung</xs:appinfo>
        </xs:annotation>
    </xs:attribute>
    <xs:attribute name="expan" type="xs:anySimpleType">
        <xs:annotation>
