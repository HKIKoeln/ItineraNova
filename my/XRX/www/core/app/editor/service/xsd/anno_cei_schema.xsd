                <caption xml:lang="de">Auflösung</caption>
            </xs:appinfo-->
            <xs:appinfo source="EditVDU">Auflösung</xs:appinfo>
        </xs:annotation>
    </xs:attribute>
    <xs:attributeGroup name="standard">
        <xs:attribute ref="id"/>
        <xs:attribute ref="n"/>
